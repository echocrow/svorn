{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "paths": {
      "#lib": ["src/lib"],
      "#lib/*": ["src/lib/*"],
      "$lib": ["src/lib"],
      "$lib/*": ["src/lib/*"]
    }
  },
  "include": ["src/**/*.js", "src/**/*.ts", "src/**/*.svelte", "tools/**/*.ts"]
}
